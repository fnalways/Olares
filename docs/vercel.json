{
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    }
  ],

  "redirects": [
    {
      "source": "/",
      "destination": "/manual/docs-home.html",
      "permanent": true
    },
    {
      "source": "/manual/olares/studio{/:(.*)}?",
      "destination": "/developer/develop/tutorial/$1",
      "permanent": true
    },
    {
      "source": "(/zh)?/developer/develop/tutorial/studio",
      "destination": "$1/developer/develop/tutorial",
      "permanent": true
    },
    {
      "source": "/developer/develop/tutorial/test",
      "destination": "/developer/develop/tutorial",
      "permanent": true
    }
  ]
}